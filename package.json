{
  "name": "mud-lint",
  "displayName": "Mud Lang Support",
  "description": "Provides rich language support for Mud, including syntax highlighting, IntelliSense, and more.",
  "version": "1.5.1",
  "publisher": "NicholasHeyer",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickheyer/mud-lint"
  },
  "license": "MIT",
  "homepage": "https://heyer.app",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "icon": "images/iconw.png",
  "galleryBanner": {
    "color": "#2b2b2b", 
    "theme": "dark"
  },
  "badges": [
    {
      "label": "Downloads",
      "url": "https://img.shields.io/visual-studio-marketplace/d/NicholasHeyer.mud-lint",
      "href": "https://marketplace.visualstudio.com/items?itemName=NicholasHeyer.mud-lint",
      "description": "Download Mud Lint"
    }
  ],
  "contributes": {
    "languages": [
      {
        "id": "mud",
        "aliases": ["Mud", "mud"],
        "extensions": [".mud", ".mudscript"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mud",
        "scopeName": "source.mud",
        "path": "./syntaxes/mud.tmLanguage.json"
      }
    ]
  },
  "main": "./src/extension.js",
  "dependencies": {}
}
