{
  "name": "mud-lint",
  "displayName": "Mud Lang Support",
  "description": "Mud language support for VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mud",
        "aliases": [
          "Mud",
          "mud"
        ],
        "extensions": [
          ".mud",
          ".mudscript"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mud",
        "scopeName": "source.mud",
        "path": "./syntaxes/mud.tmLanguage.json"
      }
    ]
  },
  "main": "./src/extension.js",
  "dependencies": {}
}
